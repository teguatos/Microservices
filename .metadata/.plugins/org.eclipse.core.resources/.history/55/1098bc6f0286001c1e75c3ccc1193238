server:
  port: 8081

spring:
  datasource:
    driverClassName: org.h2.Driver
    url: jdbc:h2:mem:testdb
  jpa:
    defer-datasource-initialization: true 
    hibernate:
      dd-auto: create
    show-sql: true
  h2:
    console:
      enabled: true
  cloud:
    config:
      import-check:
        enabled: false